	"tokenColors": [
		// ═══════════════════════════════════════════════════════════════
		// SYNTAX HIGHLIGHTING - CYBERPUNK 2077 SEMANTICS
		// PRINCIPALES: Pink = Control/Action | Cyan = Data/Variables
		// SECUNDARIOS: Yellow = Warnings | Red = Structure/Errors
		// ═══════════════════════════════════════════════════════════════
		
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7F6CFF"
			}
		},
		{
			"name": "Variables - Electric Yellow (Data)",
			"scope": [
				"variable",
				"variable.other",
				"variable.parameter",
				"variable.other.property",
				"variable.other.member",
				"variable.other.member.python",
				"variable.other.attribute",
				"meta.attribute.python",
				"meta.object-literal.key",
				"meta.property.object",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#F3E600"
			}
		},
		{
			"name": "Constants - Optical Cyan (Data)",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.escape",
				"constant.other.color"
			],
			"settings": {
				"foreground": "#55EAD4"
			}
		},
		{
			"name": "Invalid - Trauma Red",
			"scope": [
				"invalid",
				"invalid.illegal",
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#C5003C",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keywords and Control - Pink Neon (Action)",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FF006E",
				"fontStyle": "bold"
			}
		},
		{
